userfile

Endpoint UserTest;
Endpoint UserTestHelper;



Sequences {
  RequestToCert: UserTest.doNothing -> UserTestHelper.check_database;
}

Sequence RequestToCert(Text request) {
    UserTest.doNothing{}
    UserTestHelper.check_database {
        Text cert = CA.req_to_cert(request);
        print(cert);
    }
}

UserTest {
    Public Function register_user(Text request) {
        RequestToCert(request);
    }
}

UserTestHelper {
    Endpoint CA;
    onCreate(Endpoint server) {
        CA = server;
    }
}
