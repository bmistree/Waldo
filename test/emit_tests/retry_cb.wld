RetryTest
Endpoint SingleHost;


SingleHost
{
    Function (returns: Nothing) long_func;
    Number shared_state = 0;
    
    onCreate(Function (returns: Nothing) long_funcer)
    {
        long_func = long_funcer;
    }

    Public Function long_event()
    {
        shared_state += 1;
        long_func();
    }

    Public Function short_event()
    {
        shared_state += 1;
    }
}
